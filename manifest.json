{
  "name": "Unmeidum",
  "description": "UnMedium - Lightweight Core Version",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "scripting",
    "contentSettings",
    "webRequest",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "content_scripts": [
    {
      "matches": ["*://webcache.googleusercontent.com/*"],
      "js": ["content-script/enhanceGoogleCache.js"],
      "run_at": "document_start"
    }
  ]
}
